{
    "name": "ns/filtered-pagination-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides a paginated filter handler",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Nathanael d. Noblet",
            "email": "nathanael@noblet.ca"
        }
    ],
    "require": {
        "php":                          ">=7.1",
        "symfony/framework-bundle":     "^4.0|^5.0",
        "symfony/form":                 "^4.0|^5.0",
        "symfony/event-dispatcher":     "^4.0|^5.0",
        "symfony/http-foundation":      "^4.0|^5.0",
        "knplabs/knp-paginator-bundle": "^2.4|^5.0",
        "lexik/form-filter-bundle":     "^4.0|^5.0|^6.0",
        "liip/rmt": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": { "NS\\FilteredPaginationBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "NS\\FilteredPaginationBundle\\Tests\\": "Tests" }
    },
    "extra": {
        "branch-alias": {
           "dev-master": "4.0.x-dev"
        }
    }
}
